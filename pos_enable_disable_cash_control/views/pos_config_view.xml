<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="pos_config_view_form_inherit_enable_disable_cash_control" model="ir.ui.view">
    <field name="name">pos.config.form.view</field>
    <field name="model">pos.config</field>
    <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
    <field name="arch" type="xml">
      <xpath expr="(//div[@class='row mt16 o_settings_container'])[7]" position="inside">
        <div class="col-12 col-lg-6 o_setting_box">
          <div class="o_setting_left_pane">
            <field name="is_cash_control_enable_disable" invisible="0" force_save="1"/>
          </div>
          <div class="o_setting_right_pane">
            <label for="is_cash_control_enable_disable"/>
          </div>
        </div>
      </xpath>
    </field>
  </record>
</odoo>
